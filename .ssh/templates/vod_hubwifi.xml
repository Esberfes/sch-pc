<?xml version="1.0" encoding="UTF-8"?>

<template version="2.0" type="hubwifi">
	<wificonfiguration>
		<capable>
			<result>
				<expr type="and">
					<reference type="test" id="tst_segmento_not_movil" role="front"/> <!-- DCO20230508 - Movil Partis --> 
					<reference type="test" id="tst_segmento_not_Corporate" role="front"/> <!-- ARL20220307 adaptaciones para la incorporacion de corporate fijo   -->
					<reference type="test" id="tst_endpoint_grc_isok" role="front"/>
					<reference type="test" id="tst_endpoint_grc_hasresponse" role="front"/>
					<reference type="test" id="tst_service_isdiagnosticable" role="front"/>
					<reference type="test" id="tst_service_hasinternet" role="front"/>
        			<reference type="test" id="tst_productclass_not_hg553" role="cpe"/>
                	<reference type="test" id="tst_productclass_not_hg556" role="cpe"/>
        			<reference type="test" id="tst_productclass_not_arcadyan" role="cpe"/>
					<!--CCA20231123 - SP3306 - inicio-->
					<reference type="test" id="tst_plume_int_dispositivos_conectados_gt1" role="plume" inverse="true"/>
					<!--CCA20231123 - SP3306 - fin-->
				</expr>
			</result>
			<messages>
				<message id="msg_wificonfig_not_capable_cpe_hg553_arcadyan" severity="error" datatype="text" storable="true">
					<body>
						<value type="value">No se puede obtener la configuración WiFi</value>
					</body>
					<requirement>
                        <expr type="and">
							<reference type="test" id="tst_segmento_not_movil" role="front"/> <!-- DCO20230508 - Movil Partis --> 
							<reference type="test" id="tst_segmento_not_Corporate" role="front"/> <!-- TML20220510 adaptaciones para la incorporacion de corporate movil  -->
							<reference type="test" id="tst_endpoint_grc_isok" role="front"/>
							<reference type="test" id="tst_endpoint_grc_hasresponse" role="front"/>
							<reference type="test" id="tst_service_isdiagnosticable" role="front"/>
							<reference type="test" id="tst_service_hasinternet" role="front"/>
                			<reference type="test" id="tst_productclass_not_hg553" role="cpe"/>
                			<reference type="test" id="tst_productclass_not_hg556" role="cpe"/>
                			<reference type="test" id="tst_productclass_not_arcadyan" role="cpe"/>
                        </expr>
                    </requirement>
				</message>
				<message id="msg_wificonfig_not_capable" severity="error" datatype="text" storable="true">
					<body>
						<value type="value">Para configurar este equipo, accede a la web del router en remoto desde aquí:<![CDATA[<br/>]]>Vista Técnica / CPE / Datos CPE – General / Acceso a Interfaz GUI Router</value>
					</body>
					<requirement>
                        <expr type="and">
							<reference type="test" id="tst_segmento_not_movil" role="front"/> <!-- DCO20230508 - Movil Partis --> 
							<reference type="test" id="tst_segmento_not_Corporate" role="front"/> <!-- TML20220510 adaptaciones para la incorporacion de corporate movil  -->
							<reference type="test" id="tst_endpoint_grc_isok" role="front"/>
							<reference type="test" id="tst_endpoint_grc_hasresponse" role="front"/>
							<reference type="test" id="tst_service_isdiagnosticable" role="front"/>
							<reference type="test" id="tst_service_hasinternet" role="front"/>
							<expr type="or">
	                			<reference type="test" id="tst_productclass_not_hg553" role="cpe" inverse="true"/>
                				<reference type="test" id="tst_productclass_not_hg556" role="cpe" inverse="true"/>
	                			<reference type="test" id="tst_productclass_not_arcadyan" role="cpe" inverse="true"/>
	                		</expr>
                        </expr>
                    </requirement>
				</message>
				<!-- ARL20220307 adaptaciones para la incorporacion de corporate fijo - INCIO   -->
				<message id="msg_wificonfig_not_capable_corporate" severity="error" datatype="text" storable="true">
					<body>
						<value type="value">Esta herramienta no esta disponible para Servicio Corporate</value><!--CCA20220602 - Cambia el nombre de Corporativo a Corporate-->
					</body>
					<requirement>
					<expr type="and">
						<reference type="test" id="tst_segmento_not_Corporate" role="front" inverse="true"/>
					</expr>
					</requirement>
				</message>
				<!-- ARL20220307 adaptaciones para la incorporacion de corporate fijo - FIN   -->
				<!-- DCO20220426 adaptaciones para la incorporacion de corporate movil - INICIO -->
				<message id="msg_wificonfig_not_capable_corporate_movil" severity="error" datatype="text" storable="true">
					<body>
						<value type="value">Esta herramienta no esta disponible para Servicio Corporate</value><!--CCA20220602 - Cambia el nombre de Corporativo a Corporate-->
					</body>
					<requirement>
						<expr type="and">
							<reference type="test" id="tst_segmento_not_movil_empresa" role="front" inverse="true"/><!--TML20220510 - faltaba el inverse-->
						</expr>
					</requirement>
				</message>
				<!-- DCO20220426 adaptaciones para la incorporacion de corporate movil - FIN -->
				<!--CCA20231123 - SP3306 - inicio-->
				<message id="msg_superwifi_mode_integrated" severity="error" datatype="text" storable="true">
					<body>
						<value type="value">Cliente con SUPERWIFI en modo Integrado.</value>
						<value type="value"><![CDATA[<br/>]]></value>
						<value type="value">Puedes consultar el estado de los Extensores en la pestaña de Superwifi de la vista tecnica de Schaman</value>
						<value type="value"><![CDATA[<br/>]]></value>
						<value type="value">para poder gestionar los parametros wifi  es necesario acceder a la herramienta de Plume https://es.vodafone.sigma.central.plume.com/</value>
					</body>
					<requirement>
						<expr type="and">
							<reference type="test" id="tst_plume_int_dispositivos_conectados_gt1" role="plume"/>
						</expr>
					</requirement>
				</message>
				<!--CCA20231123 - SP3306 - fin-->
			</messages>
		</capable>
		<available>
			<result>
				<expr type="and">
					<reference type="test" id="tst_endpoint_grc_isok" role="front"/>
					<reference type="test" id="tst_endpoint_grc_hasresponse" role="front"/>
					<reference type="test" id="tst_service_isdiagnosticable" role="front"/>
					<reference type="test" id="tst_service_status_no_corte_diag" role="front"/>
					<reference type="test" id="tst_resource_cpe_intopology" role="front"/>
					<reference type="test" id="tst_cpe_online" role="cpe"/>
					<reference type="test" id="tst_wifi_ssids_exists" role="cpe"/>
				</expr>
			</result>
			<messages>
				<message id="msg_wificonfig_cpe_online_ko" severity="error" datatype="text" storable="true">
					<body>
						<value type="value">No ha sido posible recuperar la información de la configuración WiFi porque el CPE del cliente no está en línea</value>
					</body>
					<requirement>
                        <expr type="and">
                            <reference type="test" id="tst_cpe_online" role="cpe" inverse="true"/>
                        </expr>
                    </requirement>
				</message>
				<message id="msg_wificonfig_not_available" severity="error" datatype="text" storable="true">
					<body>
						<value type="value">No ha sido posible recuperar la información de la configuración WiFi</value>
					</body>
					<requirement>
                        <expr type="and">
                            <reference type="test" id="tst_cpe_online" role="cpe"/>
                            <reference type="test" id="tst_wifi_ssids_exists" role="cpe" inverse="true"/>
                        </expr>
                    </requirement>
				</message>
			</messages>
		</available>
		<!--DBC_Req_005_2_Inicio_Para habilitar bandsteering y acciones multiples-->
		<multiedit>
			<result>
				<expr type="and">
					<reference type="test" id="tst_multiedit_compatible" role="cpe"/>
				</expr>
			</result>
		</multiedit>
		<unifiableglobal>
			<result>
				<expr type="and">
					<reference type="test" id="tst_bandsteering_support" role="cpe"/>
					<reference type="test" id="tst_cpe_online" role="cpe"/>
				</expr>
			</result>
		</unifiableglobal>
		<unifiablebyssid>
			<result>
				<expr type="and" result="false">
				</expr>
			</result>
		</unifiablebyssid>
		<!--DBC_Req_005_2_Inicio_Para habilitar bandsteering y acciones multiples-->
		<toolrole>cpe</toolrole>

	</wificonfiguration>

	<conndevicesconfiguration>
		<capable>
			<result>
				<expr type="and">
					<reference type="test" id="tst_segmento_not_movil" role="front"/> <!-- DCO20230508 - Movil Partis --> 
					<reference type="test" id="tst_segmento_not_Corporate" role="front"/> <!-- CCA20220322 adaptaciones para la incorporacion de corporate fijo   -->
					<reference type="test" id="tst_endpoint_grc_isok" role="front"/>
					<reference type="test" id="tst_endpoint_grc_hasresponse" role="front"/>
					<reference type="test" id="tst_service_isdiagnosticable" role="front"/>
					<reference type="test" id="tst_service_hasinternet" role="front"/>
