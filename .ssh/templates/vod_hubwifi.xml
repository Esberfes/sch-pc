				</expr>
			</result>
			<messages>
				<message id="msg_conndevices_not_capable" severity="error" datatype="text" storable="true">
					<body>
						<value type="value">No se pueden obtener los dispositivos conectados</value>
					</body>
					<requirement>
					<expr type="and">
						<reference type="test" id="tst_segmento_not_movil" role="front"/> <!-- DCO20230508 - Movil Partis --> 
						<reference type="test" id="tst_segmento_not_Corporate" role="front"/>
					</expr>
					</requirement>
				</message>
				<!-- CCA2022022 adaptaciones para la incorporacion de corporate fijo - INCIO   -->
				<message id="msg_wificonfig_not_capable_corporate" severity="error" datatype="text" storable="true">
					<body>
						<value type="value">Esta herramienta no esta disponible para Servicio Corporate</value><!--CCA20220602 - Cambia el nombre de Corporativo a Corporate-->
					</body>
					<requirement>
					<expr type="and">
						<reference type="test" id="tst_segmento_not_Corporate" role="front" inverse="true"/>
					</expr>
					</requirement>
				</message>
				<!-- CCA2022022 adaptaciones para la incorporacion de corporate fijo - FIN   -->
				<!-- TML20220510 adaptaciones para la incorporacion de corporate movil - INICIO -->
				<message id="msg_conndevices_not_capable_corporate_movil" severity="error" datatype="text" storable="true">
					<body>
						<value type="value">Esta herramienta no esta disponible para Servicio Corporate</value><!--CCA20220527 - Cambia el nombre de Corporativo a Corporate-->
					</body>
					<requirement>
						<expr type="and">
							<reference type="test" id="tst_segmento_not_movil_empresa" role="front" inverse="true"/> <!-- DCO20230508 - Movil Partis --> 
						</expr>
					</requirement>
				</message>
				<!-- TML20220510 adaptaciones para la incorporacion de corporate movil - FIN -->
			</messages>
		</capable>
		<available>
			<result>
				<expr type="and">
					<reference type="test" id="tst_endpoint_grc_isok" role="front"/>
					<reference type="test" id="tst_endpoint_grc_hasresponse" role="front"/>
					<reference type="test" id="tst_service_isdiagnosticable" role="front"/>
					<reference type="test" id="tst_service_status_no_corte_diag" role="front"/>
					<reference type="test" id="tst_resource_cpe_intopology" role="front"/>
					<reference type="test" id="tst_cpe_online" role="cpe"/>
					<reference type="test" id="tst_endpoint_cpcpe_condev_isok" role="cpe"/>
					<reference type="test" id="tst_cpe_con_devices_number_of_entries_ok" role="cpe"/>
					<reference type="test" id="tst_cpe_con_devices_hay_dispositivos" role="cpe"/>
					<reference type="test" id="tst_con_condevicestable_exists" role="cpe"/>
				</expr>
			</result>
			<messages>
				<message id="msg_conndevices_cpe_online_ko" severity="error" datatype="text" storable="true">
					<body>
						<value type="value">No ha sido posible recuperar la información de los dispositivos conectados porque el CPE del cliente no está en línea</value>
					</body>
					<requirement>
                        <expr type="and">
                            <reference type="test" id="tst_cpe_online" role="cpe" inverse="true"/>
                        </expr>
                    </requirement>
				</message>
				<message id="msg_conndevices_fallo_llamada" severity="error" datatype="text" storable="true">
					<body>
						<value type="value">No ha sido posible recuperar la información de los dispositivos conectados por un fallo en la llamada</value>
					</body>
					<requirement>
                        <expr type="and">
                            <reference type="test" id="tst_cpe_online" role="cpe"/>
                            <reference type="test" id="tst_endpoint_cpcpe_condev_isok" role="cpe" inverse="true"/>
                        </expr>
                    </requirement>
				</message>
				<message id="msg_conndevices_number_of_entries_ko" severity="error" datatype="text" storable="true">
					<body>
						<value type="value">No ha sido posible recuperar la información de los dispositivos conectados porque el número de dispositivos es desconocido</value>
					</body>
					<requirement>
                        <expr type="and">
                            <reference type="test" id="tst_cpe_online" role="cpe"/>
							<reference type="test" id="tst_endpoint_cpcpe_condev_isok" role="cpe"/>
							<reference type="test" id="tst_cpe_con_devices_number_of_entries_ok" role="cpe" inverse="true"/>
                        </expr>
                    </requirement>
				</message>
				<message id="msg_conndevices_no_hay_dispositivos" severity="error" datatype="text" storable="true">
					<body>
						<value type="value">No hay dispositivos conectados</value>
					</body>
					<requirement>
                        <expr type="and">
                            <reference type="test" id="tst_cpe_online" role="cpe"/>
							<reference type="test" id="tst_endpoint_cpcpe_condev_isok" role="cpe"/>
							<reference type="test" id="tst_cpe_con_devices_number_of_entries_ok" role="cpe"/>
							<reference type="test" id="tst_cpe_con_devices_hay_dispositivos" role="cpe" inverse="true"/>
                        </expr>
                    </requirement>
				</message>
				<message id="msg_conndevices_not_available" severity="error" datatype="text" storable="true">
					<body>
						<value type="value">No ha sido posible recuperar la información de los dispositivos conectados</value>
					</body>
					<requirement>
                        <expr type="and">
                            <reference type="test" id="tst_cpe_online" role="cpe"/>
							<reference type="test" id="tst_endpoint_cpcpe_condev_isok" role="cpe"/>
							<reference type="test" id="tst_cpe_con_devices_number_of_entries_ok" role="cpe"/>
							<reference type="test" id="tst_cpe_con_devices_hay_dispositivos" role="cpe"/>
                            <reference type="test" id="tst_con_condevicestable_exists" role="cpe" inverse="true"/>
                        </expr>
                    </requirement>
				</message>
			</messages>
		</available>
		<toolrole>cpe</toolrole>
	</conndevicesconfiguration>
	
	<svaconfiguration>    
	    <capable>
			<result>
				<expr type="and">
					<reference type="test" id="tst_segmento_not_movil" role="front"/> <!-- DCO20230508 - Movil Partis --> 
					<reference type="test" id="tst_segmento_not_Corporate" role="front"/> <!-- CCA20220322 adaptaciones para la incorporacion de corporate fijo   -->
					<reference type="test" id="tst_endpoint_grc_isok" role="front"/>
					<reference type="test" id="tst_endpoint_grc_hasresponse" role="front"/>
					<reference type="test" id="tst_service_isdiagnosticable" role="front"/>
					<reference type="test" id="tst_technology_ishfc_legacy" role="front" inverse="true"/><!--CCA 20211125 - HFC Legacy -->
					<!--<reference type="test" id="tst_cliente_hfc_legacy_ono_boxless" role="front" inverse="true"/>DCL20220310 - HFCL--><!-- JJSM 20210804 -  INCHM_112_2 -->
					<!--DBC_Se comentan en pre para pruebas<reference type="test" id="tst_sip_ims_ok" role="front" inverse="true"/>DBC_Se añade para que no aplique en clientes SIP
					<reference type="test" id="tst_client_is_vodafone" role="front"/>
					<reference type="test" id="tst_service_status_no_corte_diag" role="front"/>
					<reference type="test" id="tst_service_hasphone" role="front"/>-->
				</expr>
			</result>
			<messages>
				<message id="msg_sva_not_capable" severity="error" datatype="text" storable="true">
					<body>
						<value type="value">No se pueden obtener los SVA's</value>
					</body>
					<requirement>
					<expr type="and">
						<reference type="test" id="tst_segmento_not_Corporate" role="front"/>
						<reference type="test" id="tst_segmento_not_movil" role="front"/> <!-- DCO20230508 - Movil Partis --> 
					</expr>
					</requirement>
				</message>
				<!-- CCA2022022 adaptaciones para la incorporacion de corporate fijo - INCIO   -->
				<message id="msg_wificonfig_not_capable_corporate" severity="error" datatype="text" storable="true">
					<body>
						<value type="value">Esta herramienta no esta disponible para Servicio Corporate</value><!--CCA20220602 - Cambia el nombre de Corporativo a Corporate-->
					</body>
					<requirement>
					<expr type="and">
						<reference type="test" id="tst_segmento_not_Corporate" role="front" inverse="true"/>
					</expr>
					</requirement>
				</message>
				<!-- CCA2022022 adaptaciones para la incorporacion de corporate fijo - FIN   -->
				<!-- TML20220510 adaptaciones para la incorporacion de corporate movil - INICIO -->
				<message id="msg_conndevices_not_capable_corporate_movil" severity="error" datatype="text" storable="true">
					<body>
						<value type="value">Esta herramienta no esta disponible para Servicio Corporate</value><!--CCA20220527 - Cambia el nombre de Corporativo a Corporate-->
					</body>
					<requirement>
						<expr type="and">
							<reference type="test" id="tst_segmento_not_movil_empresa" role="front" inverse="true"/> <!-- DCO20230508 - Movil Partis --> 
						</expr>
					</requirement>
				</message>
				<!-- TML20220510 adaptaciones para la incorporacion de corporate movil - FIN -->
			</messages>
	    </capable>    
	    <available>
			<result>
				<expr type="and">
					<reference type="test" id="tst_endpoint_grc_isok" role="front"/>
					<reference type="test" id="tst_endpoint_grc_hasresponse" role="front"/>
					<reference type="test" id="tst_service_isdiagnosticable" role="front"/>
					<reference type="test" id="tst_client_is_vodafone" role="front"/>
					<!--DBC_INC_Unknown_Se añade para evitar lanzamientos sin numeración para reducir la tasa de errores-->
					<reference type="test" id="tst_service_hasphone" role="front"/>
					<reference type="test" id="tst_technology_ishfc_legacy" role="front" inverse="true"/><!--DCL20220310 - HFCL-->
					<!--<reference type="test" id="tst_cliente_hfc_legacy_ono_boxless" role="front" inverse="true"/>DCL20220310 - HFCL--><!-- JJSM 20210804 -  INCHM_112_2 -->
					<!--DBC_Se comentan en pre para pruebas<reference type="test" id="tst_service_status_no_corte_diag" role="front"/>
					<reference type="test" id="tst_cims_sdd_user_registrado" role="phoneplatform"/>
					<reference type="test" id="tst_endpoint_cims_sdd_isok" role="phoneplatform"/>
					-->
					<!--DBC_INC_Unknown_Se añade para ejecutar estos endpoints y que refresque los valores de voicemail y telephone number-->
					<reference type="test" id="tst_endpoint_cfu_ok" role="phoneplatform"/>
					<reference type="test" id="tst_endpoint_cfb_ok" role="phoneplatform"/>
					<reference type="test" id="tst_endpoint_cfna_ok" role="phoneplatform"/>
				</expr>
			</result>
			<messages>
				<message id="msg_sva_not_available_llamada_ko" severity="error" datatype="text" storable="true">
