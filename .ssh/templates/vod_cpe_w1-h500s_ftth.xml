                <!-- POH 20211102 - Check para comprobar que tiene serviceID - INICIO -->               
                <reference type="check" id="chk_template_technology"/><!--MPF20210622 - INCF_013_2-->
                <reference type="check" id="chk_grc_service_id_eq_unknown" inverse="true"/> 
